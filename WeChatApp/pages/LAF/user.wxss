/* pages/LAF/user.wxss */
page{
  height: 100%;
  width: 100%;
}
.title{
  width: 100%;
  height: 5%;
  background-color: white;
}
.title_text{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}
.userinfo {
  display: flex;
  height: 40%;
  margin-bottom: 3%;
  flex-direction: column;
  align-items: center;
  border-bottom: 1rpx solid;
  border-color: wheat;
  background-repeat:no-repeat; background-size:100% 100%;-moz-background-size:100% 100%;
  background-image: url('data:image/png;base64,')
}

.userinfo-avatar {
  width: 180rpx;
  height: 180rpx;
  margin-top: 20%;
  border-radius: 50%;
}
.userinfo-nickname {
  color:white;
}
.info_v{
  width: 100%;
  height: 8%;
  background-color: white;
  border-bottom:1rpx solid;
  border-color: wheat;
}
.info_t{
  width: 94%;
  height: 100%;
  margin-left: 3%;
  margin-right: 3%;
  text-align: right;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 80%;
  color: rgb(148, 145, 145);
}
.info_imgV{
  height: 100%;
  width: 40%;
  display: flex;
  align-items: center;
}
.info_img{  
  height:60%;
  width: 15%; 
  margin-right: 5%;
}
.infoAll{
  width: 94%;
  height: 85%;
  margin-top: 3%;
  margin-left:3%;
  margin-right:3%; 
}
.infoView{
  /* background-color: white; */
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 25%;
  margin-top: 1%;
  /* border:1rpx ridge;
  border-color: rgb(220, 230, 223);
  border-radius: 5%; */
}
.OneInfo{
  width: 100%;
  height: 100%;
  display: flex;
  box-shadow: 2rpx 2rpx 5rpx #bbb;
  border-radius: 5%;
  border: 1px solid;
  border-color: white;
}
.infoImgView{
  height: 100%;
  width: 40%;
}
.infoImg{
  height: 100%;
  width: 100%;
  border-radius: 5%;
}
.infoText{
  display: flex;
  margin-left: 2%;
  flex-direction: column;
  height: 100%;
  width: 58%;
  background-size: 60%;
  /**/
  background-repeat:no-repeat;
  /**/
  background-position: 100% 0%;
  /* transform: rotate(30deg);  */
}
.infoThemeView{
  display: flex;
  flex-direction: column;
  height: 40%;
  width: 95%;
}
.infoThemeRemindView{
  height: 50%;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.infoKind{
  height: 100%;
  width: 95%;
  display: flex;
  align-items: center;
  color: red;
  /* border: 1px solid red; */
}
.infoTheme{
  height: 50%;
  width: 95%;
  display: flex;
  align-items: center;
  /* border: 1px solid red; */
}
.infoPlace{
  height: 44%;
  width: 100%;
  display: flex;
  align-items: center;
  padding-top: 2%;
}
.infoTime{
  height: 14%;
  width: 100%;
  margin-bottom: 2%;
  display: flex;
  align-items: center;
}
.infoRemind{
  padding-top: 1%;
  width: 5%;
  height: 50%;
}
