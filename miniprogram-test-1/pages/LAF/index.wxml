<view class="info">
<view class='evaluate' bindtap='openEva'>
<view class='evaluateView' >
<image class='evaluateImage' src='/pages/img/evaluate.png'></image>
<text class='evaluate_word'>评价</text>
</view>
<!-- <image class='evaluate_remind' src='/pages/img/{{openEva}}.png'></image> -->
<text class='{{openEva}} text-grey'></text> 
</view>
<block wx:if='{{evaluate}}'>
<view class='evaluateInfo'>
<text class="word">用户界面</text>
<view class="evaluateLevel">
<view wx:for="{{uiList}}" bindtap='ui' wx:for-item="ui" data-index='{{index}}' wx:key="name" class="star">
<!-- <image style='width:100%;height:100%' src="/pages/img/evaluateStar{{ui}}.png "></image> -->
<text class='icon {{ui}}  text-orange'></text>
</view>
</view>
<text style='font-size:80%;padding-left:2%;color:#8c8c8c'>{{ui}}</text>
</view>

<view class='evaluateInfo'>
<text class="word">用户体验</text>
<view class="evaluateLevel">
<view wx:for="{{feelList}}" bindtap='feel' wx:for-item="feel" data-index='{{index}}' wx:key="name" class="star">
<!-- <image style='width:100%;height:100%' src="/pages/img/evaluateStar{{feel}}.png "></image>  -->
<text class='icon {{feel}}  text-orange'></text>
</view>
</view>
<text style='font-size:80%;padding-left:2%;color:#8c8c8c'>{{feel}}</text>
</view>

<view class='evaluateInfo'>
<text class="word">有用程度</text>
<view class="evaluateLevel">
<view wx:for="{{useList}}" bindtap='use' wx:for-item="use" data-index='{{index}}' wx:key="name" class="star" >
<!-- <image style='width:100%;height:100%' src="/pages/img/evaluateStar{{use}}.png "></image>  -->
<text class='icon {{use}}  text-orange'></text>
</view>
</view>
  <text style='font-size:80%;padding-left:2%;color:#8c8c8c'>{{use}}</text>
</view>
<view class='evaluateInfo'>
<text class="word">加载速度</text>
<view class="evaluateLevel">
<view wx:for="{{loadList}}" bindtap='load' wx:for-item="load" data-index='{{index}}' wx:key="name" class="star" >
<!-- <image style='width:100%;height:100%' src="/pages/img/evaluateStar{{load}}.png "></image>  -->
<text class='icon {{load}}  text-orange'></text>
</view>
</view>
<text style='font-size:80%;padding-left:2%;color:#8c8c8c'>{{load}}</text>
</view>
<text class="advice">如果您有具体建议，请点击个人主页->意见反馈</text>
</block>
</view>
